{
 "manifest_version": 3,
  "name": "task extension",
  "version": "1.0",
  "description": "upwork task extension",
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
},
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs"
  ]
  
}